rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /receipts/{receiptId} {
      allow read, write: if true;
    }
    match /user_vaults/{vaultId} {
        allow read, write: if true;
    }
    match /notifications/{notificationId} {
        allow read, write: if true;
    }
  }
}
